    "react-dom": "^16.6.3",
    "react-resize-detector": "^3.2.1",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "typeface-roboto": "^0.0.54",
    "webrtc-adapter": "^7.1.1"
