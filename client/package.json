{
  "name": "x-core",
  "version": "1.0.0",
  "private": true,
  "repository": "https://github.com/Livestream/cloud-studio-graphics-control",
  "description": "cloud-studio-graphics-control.",
  "main": "./src/main/index.ts",
  "scripts": {
    "start": "npx cross-env NODE_ENV=production npx webpack-dev-server --config ./cli/build/config/webpack.config.ts",
    "start:dev": "npx cross-env NODE_ENV=development npx webpack-dev-server --config ./cli/build/config/webpack.config.ts --inline --inspect $NODE_DEBUG_OPTION",
    "=================================================================": "============================================",
    "build": "npx cross-env NODE_ENV=production npm run cli build-client:webpack",
    "build:dev": "npx cross-env NODE_ENV=development npm run cli build-client:webpack",
    "================================================================": "=============================================",
    "cli": "npx ts-node -P ./cli/tsconfig.json ./cli/cli.ts",
    "test": "npm run cli test"
  },
  "sideEffects": [
    "*.css",
    "*.scss"
  ],
  "babel": {
    "minified": true,
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "8",
            "browsers": [
              "last 50 versions"
            ]
          }
        }
      ],
      "@babel/preset-stage-0",
      "@babel/preset-react"
    ]
  },
  "stylelint": {
    "extends": "stylelint-config-standard",
    "plugins": [
      "stylelint-scss",
      "stylelint-order"
    ],
    "rules": {
      "order/order": [
        "custom-properties",
        "declarations"
      ],
      "order/properties-alphabetical-order": true
    }
  },
  "author": "Neloreck",
  "license": "ISC",
  "dependencies": {
    "@material-ui/core": "^3.1.0",
    "@material-ui/icons": "^3.0.1",
    "history": "^4.7.2",
